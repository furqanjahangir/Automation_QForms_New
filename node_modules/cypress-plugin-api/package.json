{
  "author": "Filip Hric (https://filiphric.com/)",
  "license": "ISC",
  "name": "cypress-plugin-api",
  "version": "2.3.1",
  "description": "UI for testing API in Cypress",
  "main": "dist/support.js",
  "types": "dist/types.d.ts",
  "files": [
    "/dist/*"
  ],
  "scripts": {
    "test": "cypress run",
    "cy:open": "cypress open",
    "cy:run": "cypress run",
    "start": "node ./server",
    "cy:run:requestMode": "cypress run --config specPattern='cypress/e2e/requestMode.cy.ts',excludeSpecPattern=null --env=requestMode=true",
    "build": "vite build",
    "build:watch": "vite build --watch",
    "lint": "tsc --noEmit",
    "semantic-release": "semantic-release"
  },
  "pre-commit": [
    "lint"
  ],
  "devDependencies": {
    "@rollup/plugin-typescript": "^8.5.0",
    "@types/node": "^18.11.8",
    "@types/prismjs": "^1.26.0",
    "@vitejs/plugin-vue": "^3.1.0",
    "autoprefixer": "^10.4.12",
    "cypress": "^10.11.0",
    "cypress-real-events": "^1.7.2",
    "cypress-recurse": "^1.23.0",
    "express": "4.17.1",
    "postcss": "^8.4.17",
    "pre-commit": "^1.2.2",
    "semantic-release": "^19.0.5",
    "start-server-and-test": "1.14.0",
    "tailwindcss": "^3.1.8",
    "typescript": "4.5.4",
    "vite": "^3.1.4"
  },
  "peerDependencies": {
    "cypress": ">=3"
  },
  "dependencies": {
    "highlight.js": "11.4.0",
    "prismjs": "^1.29.0",
    "set-cookie-parser": "^2.5.1",
    "vue": "^3.2.41"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/filiphric/cypress-plugin-api.git"
  },
  "bugs": {
    "url": "https://github.com/filiphric/cypress-plugin-api/issues"
  },
  "homepage": "https://github.com/filiphric/cypress-plugin-api#readme"
}
